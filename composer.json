{
  "name": "apiato/apiato",
  "description": "A flawless framework for building scalable and testable API-Centric Apps with PHP and Laravel.",
  "homepage": "http://apiato.io/",
  "support": {
    "issues": "https://github.com/apiato/apiato/issues",
    "source": "https://github.com/apiato/apiato"
  },
  "authors": [
    {
      "name": "Andrey Lopushansky",
      "email": "dev.magellan@gmail.com"
    }
  ],
  "keywords": [
    "Apiato",
    "API",
    "Framework",
    "API framework",
    "API Starter",
    "PHP API Starter",
    "Laravel API",
    "PHP",
    "Hello API",
    "Porto",
    "Porto SAP"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2.5",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-tokenizer": "*",
    "awobaz/compoships": "^2.0",
    "deployer/deployer": "^6.8",
    "deployer/recipes": "^6.2",
    "doctrine/dbal": "2.5.*",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^1.0",
    "genealabs/laravel-sign-in-with-apple": "^0.4.3",
    "genealabs/laravel-socialiter": "^0.2.0",
    "guzzlehttp/guzzle": "^6.3",
    "intervention/image": "^2.5",
    "laravel/framework": "^7.0",
    "laravel/passport": "~8.4.3",
    "laravel/socialite": "^4.3",
    "laravel/tinker": "^2.0",
    "laravolt/avatar": "^3.2",
    "maatwebsite/excel": "^3.2",
    "pragmarx/firewall": "^2.3",
    "prettus/l5-repository": "^2.6",
    "pusher/pusher-php-server": "^4.1",
    "rinvex/countries": "^6.1.2",
    "socialiteproviders/facebook": "^1.0",
    "socialiteproviders/manager": "^3.0",
    "socialiteproviders/odnoklassniki": "^1.0",
    "socialiteproviders/vkontakte": "^4.1",
    "spatie/laravel-fractal": "5.7.0",
    "twilio/sdk": "^6.9",
    "vinkla/hashids": "8.1.0",
    "vladimir-yuldashev/laravel-queue-rabbitmq": "^10.2.2",
    "wikimedia/composer-merge-plugin": "^1.3.1",
    "yajra/laravel-datatables-oracle": "~9.10.0"
  },
  "require-dev": {
    "facade/ignition": "^2.0",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "1.3.1",
    "nunomaduro/collision": "^4.1",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "apiato": {
      "container": {
        "name": "Authentication"
      }
    },
    "merge-plugin": {
      "include": [
        "app/Ship/composer.json",
        "app/Ship/core/composer.json",
        "app/Containers/*/composer.json",
        "app/Containers/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": true
    }
  },



  "scripts": {
 	"post-install-cmd": [
	"@php artisan vendor:publish --provider 'Prettus\\Repository\\Providers\\RepositoryServiceProvider'",
      "@php artisan vendor:publish --provider 'SocialiteProviders\\Manager\\ServiceProvider'",
      "@php artisan vendor:publish --provider='Intervention\\Image\\ImageServiceProviderLaravelRecent'"

    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --provider 'Prettus\\Repository\\Providers\\RepositoryServiceProvider'",
      "@php artisan vendor:publish --provider 'SocialiteProviders\\Manager\\ServiceProvider'",
      "@php artisan vendor:publish --provider='Intervention\\Image\\ImageServiceProviderLaravelRecent'"
    ],


    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
